{
  "name": "fh-wfm-appform",
  "version": "0.0.24",
  "description": "An appform module for WFM",
  "main": "lib/angular/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/feedhenry-raincatcher/raincatcher-appform.git"
  },
  "scripts": {
    "build": "wfm-template-build -m 'wfm.appform'",
    "watch": "wfm-template-build -w -m 'wfm.appform'",
    "test": "grunt test",
    "test-watch": "mochify --watch lib/angular/**/*-spec.js",
    "test-debug": "mochify --debug lib/angular/**/*-spec.js",
    "test-phantom": "mochify lib/angular/**/*-spec.js",
    "test-wd": "mochify --wd lib/angular/**/*-spec.js",
    "test-cover": "mochify --cover lib/angular/**/*-spec.js",
    "test-server": "node test/test-server.js | bunyan"
  },
  "keywords": [
    "wfm",
    "appform"
  ],
  "author": "Brian Leathem",
  "license": "MIT",
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "async": "1.5.2",
    "bunyan": "1.8.0",
    "config-chain": "1.1.10",
    "cors": "2.7.1",
    "dotenv": "2.0.0",
    "express": "4.13.4",
    "fh-mbaas-api": "5.14.2",
    "fh-wfm-mediator": "0.0.15",
    "fh-wfm-template-build": "0.0.9",
    "grunt": "0.4.5",
    "grunt-eslint": "18.1.0",
    "grunt-mochify": "^0.3.0",
    "jquery": "^3.1.1",
    "load-grunt-tasks": "3.5.2",
    "mochify": "2.17.0",
    "proxyquireify": "3.2.1",
    "should": "8.3.0",
    "sinon": "1.17.6",
    "sinon-as-promised": "4.0.2"
  },
  "dependencies": {
    "angular-moment": "^1.0.0",
    "fh-wfm-signature": "0.0.15",
    "lodash": "4.7.0",
    "moment": "^2.15.1",
    "q": "1.4.1"
  }
}
